{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/gemini.js": { "runtime": "nodejs18.x" }
  },
  "env": {
    "API_KEY": "@api_key"
  }
}
